REM "SOCKET TEST"
V50%=0
V1%=0
WHILE (V50%<2)
DELAY 1
FORKMCR 15,10000
DELAY 1
IF V1%=1
DELAY 1
'MOVEX A=8, M1X, P, (394.56, 15.74, 411.81, -88.26, 3.91, 5.46), R=5, H=31, MS
DELAY 3
ENDIF
IF V1%=2
DELAY 1
'MOVEX A=8, M1X, P, (394.56, 192.26, 411.81, -88.26, 3.91, 5.46), R=5, H=31, MS
DELAY 3
ENDIF
IF V1%=3
DELAY 1
'MOVEX A=8, M1X, P, (394.56, 192.26, 411.81, -88.26, 3.91, 5.46), R=5, H=31, MS
DELAY 3
ENDIF
DELAY 1
V50%=V50%+1
ENDW
DELAY 1
END